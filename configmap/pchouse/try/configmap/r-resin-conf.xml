<cluster xmlns="http://caucho.com/ns/resin"
         xmlns:resin="http://caucho.com/ns/resin/core">

<!-- import this to resin.conf :     <resin:import path="${resin.home}/conf/r-route.conf.xml"/> -->

      <env-entry>
        <description>MemCahcedClient config for RClient</description>
        <env-entry-name>memCachedClientConfig4RClient</env-entry-name>
        <env-entry-type>java.lang.String</env-entry-type>
        <env-entry-value>
			servers=172.16.0.158
            initConn=20
            minConn=10
            maxConn=50
            maintSleep=30
            nagle=false
            socketTO=3000
        </env-entry-value>
      </env-entry>

